  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.13",
    "@types/react": "19.0.10",
    "eslint": "8.57.0",
    "eslint-config-next": "15.2.2",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.0",
